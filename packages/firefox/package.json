{
  "private": "true",
  "name": "firefox",
  "version": "0.0.0",
  "scripts": {
    "start": "webpack serve"
  },
  "dependencies": {
    "bundler": "*",
    "bundlewatch": "^0.3.2"
  },
  "bundlewatch": {
    "files": [
      {
        "path": "./bundle/**/*.html"
      },
      {
        "path": "./bundle/**/*.js"
      },
      {
        "path": "./bundle/**/*.json"
      },
      {
        "path": "./bundle/**/*.svg"
      },
      {
        "path": "./bundle/**/*.woff"
      },
      {
        "path": "./bundle/**/*.mmdb"
      }
    ],
    "ci": {
      "repoBranchBase": "main"
    }
  }
}
